{
  "name": "@yundal/calendar-ui",
  "private": false,
  "version": "0.1.11",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./index.css": "./dist/index.css"
  },
  "files": [
    "dist"
  ],
  "repository": "https://github.com/yuunhae/calendar-ui.git",
  "author": "yunhae",
  "license": "MIT",
  "description": "An easy-to-use React calendar component written in TypeScript, optimized and bundled with tsup.",
  "devDependencies": {
    "@chromatic-com/storybook": "3",
    "@eslint/js": "^9.27.0",
    "@storybook/addon-actions": "^8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/experimental-addon-test": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/browser": "^3.1.4",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.27.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^0.12.0",
    "playwright": "^1.52.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "8.6.14",
    "tailwindcss": "3",
    "tailwindcss-preset-px-to-rem": "^1.2.1",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "scripts": {
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "build": "tsup src/index.ts --format esm,cjs --dts && yarn build-tailwind",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-tailwind": "npx tailwindcss -c tailwind.config.js -i ./src/lib/index.css -o ./dist/index.css --minify",
    "lint": "eslint",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,md}",
    "test": "vitest"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
